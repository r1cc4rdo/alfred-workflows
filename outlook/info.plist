			<key>config</key>
			<dict>
				<key>applescript</key>
				<string>on alfred_script(q)

    tell application "Microsoft Outlook"

        -- close reminders window if visible
        set windowClosed to false
        if "Reminder" is in (name of the front window) then
            set windowClosed to true
            close front window
        end if

		-- collect selected messages
        set messageSet to current messages
        if messageSet = {} then
            error "Select at least one message."
            error -128
        end if

        -- move the selected messages
        set archiveFolder to folder "archive" of inbox
        repeat with aMessage in messageSet
            move aMessage to archiveFolder
        end repeat

        -- re-open if we closed it
        activate
        if windowClosed is true then
            tell application "System Events" to keystroke "9" using command down
        end if

    end tell

end alfred_script</string>
